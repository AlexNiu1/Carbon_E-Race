<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Leaderboard">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="70dp"
        android:background="#E0E0E0"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent">

        <Button
            android:id="@+id/homeButton4"
            style="?android:attr/borderlessButtonStyle"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:backgroundTint="#D3D3D3"
            android:drawableTop="@drawable/ic_baseline_home_24"
            android:text="Home"
            android:textAlignment="center"
            android:textColor="#949494"
            android:textSize="10sp"
            android:textStyle="bold"
            app:rippleColor="#00FFFFFF" />

        <Button
            android:id="@+id/foodButton4"
            style="?android:attr/borderlessButtonStyle"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:backgroundTint="#D3D3D3"
            android:drawableTop="@drawable/ic_baseline_fastfood_24"
            android:text="Food"
            android:textAlignment="center"
            android:textColor="#949494"
            android:textSize="10sp"
            android:textStyle="bold"
            app:rippleColor="#00FFFFFF" />

        <Button
            android:id="@+id/travelButton4"
            style="?android:attr/borderlessButtonStyle"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:backgroundTint="#D3D3D3"
            android:drawableTop="@drawable/ic_baseline_directions_car_24"
            android:text="Travel"
            android:textAlignment="center"
            android:textColor="#949494"
            android:textSize="10sp"
            android:textStyle="bold"
            app:rippleColor="#00FFFFFF" />

        <Button
            android:id="@+id/rankButton4"
            style="?android:attr/borderlessButtonStyle"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:backgroundTint="#D3D3D3"
            android:drawableTop="@drawable/ic_baseline_leaderboard_24"
            android:text="rank"
            android:textAlignment="center"
            android:textColor="#949494"
            android:textSize="10sp"
            android:textStyle="bold"
            app:rippleColor="#00FFFFFF" />

        <Button
            android:id="@+id/settingsButton4"
            style="?android:attr/borderlessButtonStyle"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:backgroundTint="#D3D3D3"
            android:drawableTop="@drawable/ic_baseline_settings_24"
            android:text="Setting"
            android:textAlignment="center"
            android:textColor="#949494"
            android:textSize="10sp"
            android:textStyle="bold"
            app:rippleColor="#00FFFFFF" />
    </LinearLayout>
    <TextView
        android:id="@+id/fifthplace"
        android:layout_width="101dp"
        android:layout_height="0dp"
        android:layout_marginStart="55dp"
        android:layout_marginTop="426dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/fourthplace"
        android:layout_width="101dp"
        android:layout_height="0dp"
        android:layout_marginStart="55dp"
        android:layout_marginTop="388dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/thirdplace"
        android:layout_width="101dp"
        android:layout_height="0dp"
        android:layout_marginStart="55dp"
        android:layout_marginTop="356dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/secondplace"
        android:layout_width="101dp"
        android:layout_height="0dp"
        android:layout_marginStart="55dp"
        android:layout_marginTop="324dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/room"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="94dp"
        android:layout_marginTop="26dp"
        android:layout_marginEnd="200dp"
        android:layout_marginBottom="660dp"
        android:text="League: None"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

    <Button
        android:id="@+id/createleague"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="172dp"
        android:text="Start League"
        app:layout_constraintEnd_toStartOf="@+id/joinleague"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/joinleague"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="236dp"
        android:layout_marginTop="172dp"
        android:text="Join League"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/leaguename"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="Enter league name"
        android:inputType="textPersonName"
        android:minHeight="48dp"
        app:layout_constraintBottom_toTopOf="@+id/createleague"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/room" />

    <EditText
        android:id="@+id/leaguesize"
        android:layout_width="54dp"
        android:layout_height="27dp"
        android:layout_marginStart="184dp"
        android:layout_marginTop="147dp"
        android:layout_marginBottom="565dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/boardtext"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="164dp"
        android:layout_marginTop="256dp"
        android:text="Leaderboard:"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/notinleague"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="You are not in a league"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/boardtext"
        app:layout_constraintVertical_bias="0.185" />

    <Button
        android:id="@+id/leaveleague"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="156dp"
        android:layout_marginTop="172dp"
        android:layout_marginEnd="111dp"
        android:text="Leave League"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/firstplace"
        android:layout_width="101dp"
        android:layout_height="0dp"
        android:layout_marginStart="55dp"
        android:layout_marginTop="290dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>